              "raw": "{\n    \"title\": \"Quick Sort Implementation\",\n    \"description\": \"Learn and implement the quick sort algorithm.\",\n    \"descriptionHighlightTokens\": [\n        { \"token\": \"Learn and implement the \", \"isHighlighted\": false },\n        { \"token\": \"quick sort\", \"isHighlighted\": true },\n        { \"token\": \" algorithm.\", \"isHighlighted\": false }\n    ],\n    \"instructions\": \"Implement the quick sort algorithm, a highly efficient sorting algorithm based on the divide-and-conquer strategy.\",\n    \"difficulty\": \"Medium\",\n    \"tags\": [\"Sorting\", \"Algorithm\", \"Divide and Conquer\", \"Array\"],\n    \"examples\": [\n        {\n            \"input\": \"[5, 3, 8, 1, 2, 9, 4, 7, 6]\",\n            \"output\": \"[1, 2, 3, 4, 5, 6, 7, 8, 9]\",\n            \"explanation\": \"The unsorted array is sorted in ascending order.\"\n        }\n    ],\n    \"constraints\": [\n        \"1 <= arr.length <= 50000\",\n        \"-10^4 <= arr[i] <= 10^4\"\n    ]\n}"
            },
